@article{kroeker2010,
  title = {Meta-Analysis Reveals Negative yet Variable Effects of Ocean Acidification on Marine Organisms},
  author = {Kroeker, Kristy J. and Kordas, Rebecca L. and Crim, Ryan N. and Singh, Gerald G.},
  year = {2010},
  volume = {13},
