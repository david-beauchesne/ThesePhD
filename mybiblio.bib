  shorttitle = {Effects of Fishing and Predation in a Heavily Exploited Ecosystem},
  author = {Savenkoff, C. and Swain, D. P. and Hanson, J. M. and Castonguay, M. and Hammill, M. O. and Bourdages, H. and Morissette, L. and Chabot, D.},
