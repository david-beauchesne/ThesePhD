  language = {en},
  number = {9}
}

@article{gonzalez2009,
  title = {The {{Causes}} and {{Consequences}} of {{Compensatory Dynamics}} in {{Ecological Communities}}},
  author = {Gonzalez, Andrew and Loreau, Michel},
  year = {2009},
  volume = {40},
