  file = {/Users/davidbeauchesne/Zotero/storage/R4FUV6RG/Galic et al. - 2018 - When things don't add up quantifying impacts of m.pdf;/Users/davidbeauchesne/Zotero/storage/GT5I7RU3/ele.html},
  journal = {Ecology Letters},
  keywords = {Antagonism,detritivores,ecosystem services,energy budgets,individual-based model,population dynamics,Read again,Skimmed,stressor interactions,synergism},
