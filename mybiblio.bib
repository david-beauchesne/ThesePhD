  author = {Ripple, William J. and Newsome, Thomas M. and Wolf, Christopher and Dirzo, Rodolfo and Everatt, Kristoffer T. and Galetti, Mauro and Hayward, Matt W. and Kerley, Graham I. H. and Levi, Taal and Lindsey, Peter A. and Macdonald, David W. and Malhi, Yadvinder and Painter, Luke E. and Sandom, Christopher J. and Terborgh, John and Valkenburgh, Blaire Van},
