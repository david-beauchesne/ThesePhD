@article{gavrilchuk2014,
  title = {Trophic Niche Partitioning among Sympatric Baleen Whale Species Following the Collapse of Groundfish Stocks in the {{Northwest Atlantic}}},
  author = {Gavrilchuk, Katherine and Lesage, V{\'e}ronique and Ramp, Christian and Sears, Richard and B{\'e}rub{\'e}, Martine and Bearhop, Stuart and Beauplet, Gw{\'e}na{\"e}l},
